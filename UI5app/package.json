{
  "name": "csgo-dashboard",
  "version": "0.2.0",
  "description": "CSGO Dashboard",
  "private": true,
  "scripts": {
    "start": "ui5 --port 1338 serve",
    "lint": "eslint webapp",
    "karma": "karma start",
    "karma-ci": "rimraf coverage && karma start karma-ci.conf.js",
    "watch": "npm run karma",
    "test": "npm run lint && npm run karma-ci",
    "build": "ui5 build -a --clean-dest",
    "build-self-contained": "ui5 build self-contained -a --clean-dest",
    "serve-dist": "ws --compress -p 1338 -d dist"
  },
  "dependencies": {
    "@openui5/sap.m": "^1.71.3",
    "@openui5/sap.f": "^1.71.3",
    "@openui5/sap.ui.core": "^1.71.3",
    "@openui5/sap.tnt": "^1.71.3",
    "@openui5/themelib_sap_fiori_3": "^1.71.3"
  },
  "devDependencies": {
    "@ui5/cli": "^1.11.1",
    "eslint": "^5.16.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-ui5": "^1.1.0",
    "local-web-server": "^3.0.7",
    "rimraf": "^3.0.0"
  }
}
