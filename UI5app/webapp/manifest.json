{
	"_version": "1.12.0",
	"sap.app": {
		"id": "csgo.dashboard",
		"type": "application",
		"dataSources": {
			"splitpage_data": {
				"uri": "model/splitpage.json",
				"type": "JSON"
			},
			"practiceServer_data": {
				"uri": "../servers/practice/query",
				"type": "JSON"
			}
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.tnt": {}
			}
		},
		"rootView": {
			"viewName": "csgo.dashboard.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "csgo.dashboard.i18n.i18n"
				}
			},
			"splitpage": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "splitpage_data"
			},
			"practice": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "practiceServer_data"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/styles.css"
				}
			]
		}
	}
}
